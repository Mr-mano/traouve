<div class="card object-card">
    <span class="badge badge-primary badge-lost"><i class="fa fa-map-pin"></i> Perdu</span>
    <img class="card-img-top" src="uploads/{{ traobject.picture }}" alt="{{ traobject.title }}">
    <div class="card-body">
        <h5 class="card-title">{{ traobject.title }}</h5>
        <ul class="object-features">
            <li><a href="#" class="badge" style="background-color: #dccd14;"><i
                            class="fa fa-money"></i>{{ traobject.category.label }}</a></li>
            <li><a href="#" class="badge badge-info"><i class="fa fa-calendar"></i>{{ traobject.eventAt | date("d/m/Y") }}</a>
            </li>
            <li><a href="#" class="badge badge-info"><i class="fa fa-map-marker"></i>{{ traobject.city }}</a>
            </li>
        </ul>
    </div>
    <a href="{{ path('traobject_show', {'id': traobject.id}) }}" class="card-footer-btn btn btn-primary"><i class="fa fa-eye"></i> Afficher</a>
</div>