{% extends 'base.html.twig' %}

{% block title %}Traobject{% endblock %}

{% block body %}



        <div class="row">
            <div class="container col-sm-8 text-center">
                <h1>Object</h1>
                <div class="container">
                <table class="table text-center">
                    <tbody>


                        <img src="{{ asset ( 'uploads/' ~ traobject.picture) }}" alt="{{ traobject.title }}">

                    <tr>
                        <th>Titre</th>
                        <td>{{ traobject.title }}</td>
                    </tr>
                    <tr>
                        <th>Description</th>
                        <td>{{ traobject.description }}</td>
                    </tr>
                    <tr>
                        <th>Perdu le</th>
                        <td>{{ traobject.eventAt ? traobject.eventAt|date('Y-m-d') : '' }}</td>
                    </tr>
                    <tr>
                        <th>Retrouv√© le</th>
                        <td>{{ traobject.dateEnd ? traobject.dateEnd|date('Y-m-d') : '' }}</td>
                    </tr>
                    <tr>
                        <th>Adresse</th>
                        <td>{{ traobject.address }}</td>
                    </tr>
                    <tr>
                        <th>Ville</th>
                        <td>{{ traobject.city }}</td>
                    </tr>

                    </tbody>
                </table>

                    <a href="{{ path('traobject_index') }}">back to list</a>

                    <a href="{{ path('traobject_edit', {'id': traobject.id}) }}">edit</a>

                    {{ include('traobject/_delete_form.html.twig') }}

            </div>
        </div>
    </div>


{% endblock %}
